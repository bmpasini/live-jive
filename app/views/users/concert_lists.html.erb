<% provide(:title, "My Concert Lists") %>
<div class="row">
	<aside class="col-md-4">
      <section class="user_info">
        <%= render 'users/gravatar_my_concert_lists' %>
      </section>
      <section class="stats">
        <%= render 'users/stats' %>
      </section>
      <section>
        <%= render 'users/profile' %>
      </section>
    </aside>
	<div class="col-md-8">
		<% if user_or_band == "User" %>
			<%= render 'users/new_concert_list' %>
		<% end %>
		<%= render 'users/concert_lists' %>
	</div>
</div>